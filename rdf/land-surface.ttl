  skos:prefLabel "Microrelief"@en ;
.
ls:microrelief-A
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-A"^^xsd:anyURI ;
  skos:definition "discontinuous, elongate mounds and/or elongate depressions more or less at right angles to the contour. Usually in sloping lands, commonly between linear gilgai on lower slopes and plains."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "A" ;
  skos:prefLabel "Lattice gilgai"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-C
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-C"^^xsd:anyURI ;
  rdfs:comment "Vertical interval usually < 0.3 m and horizontal interval 3 - 20m"@en ;
  skos:definition "irregularly distributed, small depressions and mounds separated by a more or less continuous shelf. Vertical interval usually less than 0.3 m. Horizontal interval usually 3–20 m, surface almost level."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "C" ;
  skos:prefLabel "Crabhole gilgai"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-D
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-D"^^xsd:anyURI ;
  skos:definition "small hummocks rising above a planar surface. They vary from rounded, both planar and vertically, to flat-topped, relatively steep-sided and elongate. They are usually closely and regularly spaced, ranging from 0.06 to 0.6 m in both vertical and horizontal dimensions. They are common in northern Australia on soils with impeded internal drainage and in areas of short seasonal ponding. Many observers consider them to be formed by biological activity."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "D" ;
  skos:prefLabel "Debil-debil"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-G
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-G"^^xsd:anyURI ;
  skos:definition "irregularly distributed, large depressions, usually greater than 3 m in diameter or greatest dimension, subcircular or irregular and varying from closely spaced in a network of elongate mounds to isolated depressions set in an undulating shelf with occasional small mounds. Some depressions may also contain sinkholes of gilgai usually greater than 0.3 m; horizontal interval usually 5–6 m; surface almost level."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "G" ;
  skos:prefLabel "Contour gilgai"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-H
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-H"^^xsd:anyURI ;
  skos:definition "Depression associated with water flowing from rock or soil without the agency of man."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "H" ;
  skos:prefLabel "Spring hollow"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-I
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-I"^^xsd:anyURI ;
  skos:definition "closed depression with vertical or funnel-shaped sides."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "I" ;
  skos:prefLabel "Sinkhole"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-K
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-K"^^xsd:anyURI ;
  skos:definition "depressions in limestone country."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "K" ;
  skos:prefLabel "Karst microrelief"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-L
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-L"^^xsd:anyURI ;
  rdfs:comment "Vertical interval usually < 0.3 m and horizontal interval 5 - 8 m"@en ;
  skos:definition "long, narrow, parallel, elongate mounds and broader elongate depressions more or less at right angles to the contour. Usually in sloping lands. Vertical interval usually less than 0.3 m, horizontal interval usually 5–8 m."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "L" ;
  skos:prefLabel "Linear gilgai"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-M
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-M"^^xsd:anyURI ;
  skos:definition "irregularly distributed, large depressions, usually greater than 3 m in diameter or greatest dimension, subcircular or irregular and varying from closely spaced in a network of elongate mounds to isolated depressions set in an undulating shelf with occasional small mounds. Some depressions may also contain sinkholes of gilgai usually greater than 0.3 m; horizontal interval usually 5–6 m; surface almost level."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "M" ;
  skos:prefLabel "Melonhole gilgai"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-N
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-N"^^xsd:anyURI ;
  rdfs:comment "Vertical interval usually < 0.3 m and horizontal interval 3 - 10m"@en ;
  skos:definition "irregularly distributed, small mounds and subcircular depressions varying in size and spacing. Vertical interval usually less than 0.3 m, horizontal interval usually 3–10 m, surface almost level."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "N" ;
  skos:prefLabel "Normal gilgai"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-P
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-P"^^xsd:anyURI ;
  skos:definition "Mound associated with water flowing from rock or soil without the agency of man."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "P" ;
  skos:prefLabel "Spring mound"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-R
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-R"^^xsd:anyURI ;
  skos:definition "small terraces on slopes resulting either from soil creep or trampling by hoofed animals."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "R" ;
  skos:prefLabel "Terracettes"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-S
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-S"^^xsd:anyURI ;
  skos:definition "hummocky microrelief on the surface of landslides, slumps, earth flows, debris avalanches."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "S" ;
  skos:prefLabel "Mass movement microrelief"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-T
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-T"^^xsd:anyURI ;
  skos:definition "trenches typically 0.2 m deep and 0.6 m wide, with near vertical walls, alternating with flat-crested ridges about 1.3 m wide, which extend along the contour for several metres or tens of metres. Contour trenches are known in areas in south-eastern Australia above 350 m altitude with a high effective rainfall, where they are associated with a grassland or heathland vegetation on undulating rises (compare with McElroy 1952)."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "T" ;
  skos:prefLabel "Contour trench"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-U
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-U"^^xsd:anyURI ;
  skos:definition "undifferentiated, irregularly distributed or isolated mounds and/or depressions set in a planar surface."@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "U" ;
  skos:prefLabel "Mound/depression microrelief"@en ;
  skos:topConceptOf asls:land-surface ;
.
ls:microrelief-W
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/land-surface/microrelief-W"^^xsd:anyURI ;
  skos:definition "steep-sided hummocks rising above a flat surface. Hummocks are frequently occupied by trees or shrubs while the lower surface may be vegetation free or occupied by sedges or reeds. They are subject to prolonged seasonal flooding. "@en ;
  skos:inScheme asls:land-surface ;
  skos:notation "W" ;
  skos:prefLabel "Swamp hummock"@en ;
  skos:topConceptOf asls:land-surface ;
.
